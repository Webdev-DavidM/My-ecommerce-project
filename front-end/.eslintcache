ProductItem.js",["194"],"import React from 'react';\nimport styles from './ProductItem.module.css';\nimport { Link } from 'react-router-dom';\nimport { noAuto } from '@fortawesome/fontawesome-svg-core';\n\nconst ProductItem = (props) => {\n  let { images, _id, name, price, stock } = props.details;\n  let mainImage = images[0];\n  return (\n    <div className={styles.productitem}>\n      <div>\n        <Link to={`/product/${_id}`}>\n          {' '}\n          <img src={`http://localhost:5000${mainImage}`} alt='' />\n        </Link>\n      </div>\n      <div className={styles.productdetails}>\n        <div>\n          <Link to={`/product/${_id}`} style={{ textDecoration: 'none' }}>\n            <p className={styles.name}>{name}</p>\n          </Link>\n          <p className={styles.price}>£{price}</p>\n          <p className={styles.reviews}>reviews will go here</p>\n          {stock === 0 && (\n            <h2 className={styles.outofstock}>Sorry out of stock</h2>\n          )}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default ProductItem;\n","/Users/davidmulholland/Documents/My-full-stack-apps/E-commerce/front-end/src/Components/UIelements/MobileProductFilterMenu.js",[],"/Users/davidmulholland/Documents/My-full-stack-apps/E-commerce/front-end/src/Components/UIelements/ProductFilters.js",[],"/Users/davidmulholland/Documents/My-full-stack-apps/E-commerce/front-end/src/Components/UIelements/ProductImageCarousel.js",[],"/Users/davidmulholland/Documents/My-full-stack-apps/E-commerce/front-end/src/Components/UIelements/Reviews.js",[],"/Users/davidmulholland/Documents/My-full-stack-apps/E-commerce/front-end/src/Components/UIelements/Filters/PriceRangeFilter.js",[],"/Users/davidmulholland/Documents/My-full-stack-apps/E-commerce/front-end/src/Components/UIelements/Filters/StockFilter.js",[],"/Users/davidmulholland/Documents/My-full-stack-apps/E-commerce/front-end/src/Components/UIelements/Filters/BrandFilter.js",[],"/Users/davidmulholland/Documents/My-full
s}>\n          <span>ID: 1111111</span>\n          <span>User: David Mulholland</span>\n          <span>Date: 2/3/2019</span>\n          <span>Total: £1999</span>\n          <span>\n            Paid : <FontAwesomeIcon icon={faCheck} />\n            <FontAwesomeIcon icon={faTimes} />\n          </span>\n        </div>\n        <div className={styles.orderdetails}>\n          <span>ID: 1111111</span>\n          <span>User: David Mulholland</span>\n          <span>Date: 2/3/2019</span>\n          <span>Total: £1999</span>\n          <span>\n            Paid : <FontAwesomeIcon icon={faCheck} />\n            <FontAwesomeIcon icon={faTimes} />\n          </span>\n        </div>\n        <div className={styles.orderdetails}>\n          <span>ID: 1111111</span>\n          <span>User: David Mulholland</span>\n          <span>Date: 2/3/2019</span>\n          <span>Total: £1999</span>\n          <span>\n            Paid : <FontAwesomeIcon icon={faCheck} />\n            <FontAwesomeIcon icon={faTimes} />\n          </span>\n        </div>\n        <div className={styles.orderdetails}>\n          <span>ID: 1111111</span>\n          <span>User: David Mulholland</span>\n          <span>Date: 2/3/2019</span>\n          <span>Total: £1999</span>\n          <span>\n            Paid : <FontAwesomeIcon icon={faCheck} />\n            <FontAwesomeIcon icon={faTimes} />\n          </span>\n        </div>\n        <div className={styles.orderdetails}>\n          <span>ID: 1111111</span>\n          <span>User: David Mulholland</span>\n          <span>Date: 2/3/2019</span>\n          <span>Total: £1999</span>\n          <span>\n            Paid : <FontAwesomeIcon icon={faCheck} />\n            <FontAwesomeIcon icon={faTimes} />\n          </span>\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default AdminOrders;\n","/Users/davidmulholland/Documents/My-full-stack-apps/E-commerce/front-end/src/Pages/AdminEditProduct.js",[],"/Users/davidmulholland/Documents/My-full-stack-apps/E-commerce/front-end/src/Pages/AdminCreateProduct.js",[],"/Users/davidmulholland/Documents/My-full-stack-apps/E-commerce/front-end/src/Pages/Checkout.js",["191","192"],"/* NPM packages */\nimport React, { Component } from 'react';\nimport { connect } from 'react-redux';\n\n/* CSS */\nimport styles from './Checkout.module.css';\n\n/* Action creators */\n\nimport { addToBasket } from '../Actions/products.js';\n\nclass Checkout extends Component {\n  componentDidMount = () => {\n    let { basket, addBasket } = this.props;\n    // here we check if there is something in the basket which means the user has come\n    // from the product screen, if so then add to local storage\n    if (basket.length !== 0) {\n      let localStorageItems = Object.keys(localStorage);\n      localStorageItems.map((item) => {\n        if (item !== 'userInfo') {\n          return localStorage.removeItem(`${item}`);\n        }\n      });\n      basket.map((item, index) => {\n        return localStorage.setItem(`item${index}`, JSON.stringify(item));\n      });\n    }\n\n    // here we check if the page has been refreshed and there is nothing in the basket, if there\n    // is an item in local storage then dispatch it to be added to basket in redux so it can populate this screen\n    let localStorageItems = Object.keys(localStorage);\n    let itemsFromLocalStorage = localStorageItems.map((item) => {\n      return JSON.parse(localStorage.getItem(`${item}`));\n    });\n\n    if (basket.length === 0 && itemsFromLocalStorage)\n      // Here I will store the basket in local storage and reload if the user refreshes the page,\n      // I am mapping over the array and adding each item at a time which is what the reducer is expecting\n      itemsFromLocalStorage.map((item) => {\n        if (item['firstName'] === undefined) {\n          addBasket(item);\n        }\n      });\n  };\n\n  render() {\n    let { firstName, lastName, address } = this.props.userDetails;\n    let { basket } = this.props;\n    console.log(basket);\n\n    return (\n      <div className={styles.checkoutcontainer}>\n        <h2>\n          <span\n            className={styles.gobackbtn}\n            onClick={() => this.props.history.goBack()}>\n            Go back\n          </span>\n        </h2>\n        <h2>Summary</h2>\n        <hr></hr>\n\n        <div className={styles.deliveryaddress}>\n          <h4>Delivery</h4>\n          <span>\n            <strong> Delivering to:</strong>&nbsp;{address}\n          </span>\n          <h4>Name</h4>\n          <span>\n            <strong> Delivering to:</strong>&nbsp; {firstName} {lastName}\n          </span>\n          <h4>Payment method</h4>\n          <span>paypal and stripe needs to happen here</span>\n        </div>\n        <div className={styles.paymentmethod}></div>\n        <div className={styles.ordersummary}>\n          <div className={styles.itemcontainer}>\n            {basket.map((item) => (\n              <div className={styles.details}>\n                <div className={styles.imagecontainer}>\n                  <img\n                    src={`${process.env.PUBLIC_URL}/images/cycle-slide1.jpg`}\n                    alt=''\n                  />\n                </div>\n\n                <div className={styles.itemdetails}>\n                  <span>{item.name}</span>\n                  <br />\n                  <span>Size:{item.size}</span>\n                  <br />\n                  <span>Price: £{item.price}</span>\n                  <br />\n                </div>\n              </div>\n            ))}\n\n            <div className={styles.priceandquanity}></div>\n            <h3 className={styles.itemcost}>Total: £1999</h3>\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n\nconst mapStateToProps = (state) => {\n  return {\n    userDetails: state.user.user,\n    basket: state.products.basket,\n    basketTotal: () => {\n      let Total = 0;\n      if (state.products.basket.length !== 0) {\n        Total = state.products.basket\n          .map((product) => {\n            return product.price * product.quantity;\n          })\n          .reduce((total, itemTotal) => {\n            return total + itemTotal;\n          });\n      }\n      return { Total };\n    },\n  };\n};\n\nconst mapDispatchToProps = (dispatch) => {\n  return {\n    addBasket: (item) => dispatch(addToBasket(item)),\n  };\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Checkout);\n","/Users/davidmulholland/Documents/My-full-stack-apps/E-commerce/front-end/src/reducers/ordersReducer.js",[],"/Users/davidmulholland/Documents/My-full-stack-apps/E-commerce/front-end/src/Actions/users.js",[],"/Users/davidmulholland/Documents/My-full-stack-apps/E-commerce/front-end/src/Actions/products.js",[],"/Users/davidmulholland/Documents/My-full-stack-apps/E-commerce/front-end/src/Components/UIelements/Filters/CustomerReviewFilter.js",[],{"ruleId":"193","replacedBy":"194"},{"ruleId":"195","replacedBy":"196"},{"ruleId":"197","severity":1,"message":"198","line":22,"column":24,"nodeType":"199","messageId":"200","endLine":22,"endColumn":34},{"ruleId":"197","severity":1,"message":"201","line":73,"column":11,"nodeType":"199","messageId":"200","endLine":73,"endColumn":23},{"ruleId":"202","severity":1,"message":"203","line":30,"column":36,"nodeType":"204","messageId":"205","endLine":30,"endColumn":38},{"ruleId":"202","severity":1,"message":"206","line":50,"column":40,"nodeType":"204","messageId":"207","endLine":50,"endColumn":42},{"ruleId":"197","severity":1,"message":"208","line":66,"column":11,"nodeType":"199","messageId":"200","endLine":66,"endColumn":17},{"ruleId":"197","severity":1,"message":"209","line":66,"column":19,"nodeType":"199","messageId":"200","endLine":66,"endColumn":23},{"ruleId":"202","severity":1,"message":"203","line":90,"column":35,"nodeType":"204","messageId":"205","endLine":90,"endColumn":37},{"ruleId":"193","replacedBy":"210"},{"ruleId":"195","replacedBy":"211"},{"ruleId":"197","severity":1,"message":"212","li
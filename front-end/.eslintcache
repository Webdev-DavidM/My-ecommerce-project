ate({ showButton: true });\n    }, 11000);\n  };\n\n  render() {\n    let style = this.state.moveright ? styles.carouselmoveright : '';\n    return (\n      <>\n        <div className={`${styles.carousel} ${style}`}>\n          {this.props.url.map((image, index) => {\n            return (\n              <div key={index}>\n                <span>\n                  {index === 0 ? (\n                    <span>{this.props.cat.toUpperCase()}</span>\n                  ) : (\n                    <span>2021 RANGE IN STOCK</span>\n                  )}\n                </span>\n                <img src={`${process.env.PUBLIC_URL}/images/${image}`} alt='' />\n              </div>\n            );\n          })}\n        </div>\n      </>\n    );\n  }\n}\n","/Users/davidmulholland/Documents/My-full-stack-apps/E-commerce/front-end/src/Pages/Products.js",[],"/Users/davidmulholland/Documents/My-full-stack-apps/E-commerce/front-end/src/Components/UIelements/ProductItem.js",["192"],"import React from 'react';\nimport styles from './ProductItem.module.css';\nimport { Link } from 'react-router-dom';\nimport { noAuto } from '@fortawesome/fontawesome-svg-core';\n\nconst ProductItem = (props) => {\n  let { images, _id, name, price, stock } = props.details;\n  let mainImage = images[0];\n  return (\n    <div className={styles.productitem}>\n      <div>\n        <Link to={`/product/${_id}`}>\n          {' '}\n          <img src={`http://localhost:5000${mainImage}`} alt='' />\n        </Link>\n      </div>\n      <div className={styles.productdetails}>\n        <div>\n          <Link to={`/product/${_id}`} style={{ textDecoration: 'none' }}>\n            <p className={styles.name}>{name}</p>\n          </Link>\n          <p className={styles.price}>£{price}</p>\n          <p className={styles.reviews}>reviews will go here</p>\n          {stock === 0 && (\n            <h2 className={styles.outofstock}>Sorry out of stock</h2>\n          )}\n        </div>\n      </div>\n    </div>\n  );\n};\n\